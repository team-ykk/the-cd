<div class="container">
	<div class="row">
		<div class="col">
				<h2>顧客一覧</h2>
		</div>
	</div>
	<div class="row">
		<div class="col">
			<h3>登録ユーザー</h3>
			<table class="table table-bordered">
				<thead>
				    <tr>
				      <th colspan="1"></th>
				      <th>名前</th>
				      <th>フリガナ</th>
				      <th>メールアドレス</th>
				      <th colspan="4">
				    </tr>
				</thead>
				<tbody>
		          <% @endusers.each do |f| %>
		            <tr>
	            	<td>
	            	</td>
		              <td>
		              	<%= f.first_name %><%= f.last_name %>
		              </td>
		              <td>
		              	<%= f.first_name_kana %><%= f.last_name_kana %>
		              </td>
		              <td>
		                <%= f.email %>
		              </td>
		              <td>
		              	<%= link_to'編集',edit_admins_enduser_path(f.id) %>
		              </td>
		            </tr>
		          <% end %>
		        </tbody>
            </table>
        </div>
	</div>
</div>
						